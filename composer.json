{
    "name": "bacart/common",
    "description": "Bacart common utils",
    "type": "library",
    "keywords": [
        "php",
        "common",
        "bacart"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10.3"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "files": ["src/functions.php"],
        "psr-4": {
            "Bacart\\Common\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Alex Bacart",
            "email": "alex@bacart.net"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
